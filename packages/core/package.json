{
  "name": "@figma-vars/core",
  "version": "0.1.0",
  "type": "module",
  "description": "Core functionality for working with Figma variables",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "rimraf dist && tsc --outDir dist/types --declaration --emitDeclarationOnly && tsup",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "lint": "eslint \"src/**/*.ts*\"",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitest/coverage-v8": "^1.6.1",
    "eslint": "^8.54.0",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.2",
    "typescript": "^5.8.3",
    "vitest": "^1.6.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ipaintcode/figma-vars.git",
    "directory": "packages/core"
  },
  "keywords": [
    "figma",
    "variables",
    "design",
    "design-system",
    "tokens",
    "design-tokens"
  ],
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ]
}