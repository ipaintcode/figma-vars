{
  "name": "@figma-vars/core",
  "version": "0.1.0",
  "description": "Core functionality for Figma Variables API.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./plugin": {
      "types": "./dist/plugin/plugin.d.ts",
      "import": "./dist/plugin/plugin.js",
      "require": "./dist/plugin/plugin.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && tsc --outDir dist/types --declaration --emitDeclarationOnly && tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "figma",
    "variables",
    "design-tokens",
    "api"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@figma/plugin-typings": "^1.82.0"
  },
  "devDependencies": {
    "@types/node": "^20.17.30",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  }
}