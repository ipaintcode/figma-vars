{
  "name": "@figma-vars/core",
  "version": "0.1.0",
  "description": "Core functionality for working with Figma variables",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./plugin": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "rimraf dist && tsc --outDir dist/types --declaration --emitDeclarationOnly && tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "lint": "eslint \"src/**/*.ts*\"",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "eslint": "^8.54.0",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/figma-vars.git",
    "directory": "packages/core"
  },
  "keywords": [
    "figma",
    "variables",
    "design",
    "design-system",
    "tokens",
    "design-tokens"
  ],
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ]
}