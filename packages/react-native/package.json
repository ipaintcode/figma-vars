{
  "name": "@figma-vars/react-native",
  "version": "0.1.0",
  "description": "React Native hooks for Figma Variables",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "lint": "eslint \"src/**/*.ts*\"",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-native": "^0.73.0"
  },
  "dependencies": {
    "@figma-vars/core": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@types/react-native": "^0.73.0",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  },
  "publishConfig": {
    "access": "public"
  }
}